export function void main() {
  int x = 1;
  int y = 1;
  int z = 1;
  int c = 4;

  while (x < c) {
    while (y < c) {
      while (z < c) {
        print(x);
        print(", ");
        print(y);
        print(", ");
        print(z);
        print("\n");
        z = z + 1;
      }
      y = y + 1;
      z = 1;
    }
    x = x + 1;
    y = 1;
  }

}

