struct String {
  array int value;
  int length;
  String next;
  bool pooled;
}

export function void main() {
  string a = itos_(0);
  print(a);
}

function string itos_(int a) {
  bool is_neg = a < 0;
  int size = 0;

  if(a < 0) {
    a = a * (0 - 1);
  }

  int val = a;

  if(val == 0) {
    size = 1;
  } else{
    while(val > 0) {
      size = size + 1;
      val = val / 10;
    }
  }

  if(is_neg) {
    size = size + 1;
  }

  //   print("====\n");
  // print(size);
  // print("\n====\n");

  array int result = new int[size];

  if(is_neg) {
    result[0] = 45;
  }

  val = a;

  int index = 0;

  if(val == 0) {
    result[0] = 48;
  }else{
    while(val > 0) {
      result[size - (index + 1)] = 48 + (val % 10);
      index = index + 1;
      val = val / 10;
    }
  }



  return new String(result, size, null, false);
}